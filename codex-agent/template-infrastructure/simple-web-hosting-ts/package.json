{
  "name": "simple-web-hosting-ts",
  "version": "1.0.0",
  "description": "AWS S3 web hosting with CloudFront using CDKTF TypeScript",
  "main": "main.ts",
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "pnpm run build && node dist/main.js",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "deploy": "ts-node deploy.ts deploy",
    "destroy": "ts-node deploy.ts destroy",
    "plan": "ts-node deploy.ts plan"
  },
  "engines": {
    "node": ">=16.13"
  },
  "keywords": [
    "cdktf",
    "terraform",
    "aws",
    "s3",
    "cloudfront",
    "web-hosting"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cdktf/provider-aws": "^19.0.0",
    "cdktf": "^0.20.0",
    "constructs": "^10.0.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.0.0",
    "cdktf-cli": "^0.20.0",
    "jest": "^29.4.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.9.0"
  }
}